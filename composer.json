{
    "name": "vitexsoftware/datatables-abraflexi",
    "description": "Show AbarFlexi evidence data as DataTable",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "AbraFlexi\\ui\\DataTables\\": "src/AbraFlexi/ui/DataTables/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "vitexsoftware/abraflexi-bricks": "^1.4",
        "vitexsoftware/ease-bootstrap5-widgets-abraflexi": "^1.4"
    },
    "require-dev": {
        "datatables/datatables": "^1.10",
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.84",
        "ergebnis/composer-normalize": "^2.47",
        "ergebnis/php-cs-fixer-config": "^6.51",
        "phpstan/phpstan-phpunit": "2.0.7"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
